name: cadlab
version: {{ version }}

channels:
  - {{ build_channel }}
  - https://conda.anaconda.org/conda-forge
  - https://repo.anaconda.com/pkgs/main
  - https://repo.anaconda.com/pkgs/free

specs:
  - bqplot
  - cadlab =={{ version }}
  - cadquery
  - conda
  - importnb
  - itkwidgets
  - nodejs >={{ node_min }},<{{ node_max }}
  - numpy-stl
  - openmdao
  - petsc
  - petsc4py
  - pyoptsparse
  - python >={{ py_min }},<{{ py_max }}
  - pythreejs

install_in_dependency_order: True
